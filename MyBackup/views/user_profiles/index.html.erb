<p id="notice"><%= notice %></p>

<h1>User Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Username</th>
      <th>Useremail</th>
      <th>Userphone</th>
      <th>Userip</th>
      <th>Userlocation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_profiles.each do |user_profile| %>
      <tr>
        <td><%= user_profile.userID %></td>
        <td><%= user_profile.userName %></td>
        <td><%= user_profile.userEmail %></td>
        <td><%= user_profile.userPhone %></td>
        <td><%= user_profile.userIP %></td>
        <td><%= user_profile.userLocation %></td>
        <td><%= link_to 'Show', user_profile %></td>
        <td><%= link_to 'Edit', edit_user_profile_path(user_profile) %></td>
        <td><%= link_to 'Destroy', user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Profile', new_user_profile_path %>
