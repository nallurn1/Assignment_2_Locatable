<p id="notice"><%= notice %></p>

<h1>User Locations</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_locations.each do |user_location| %>
      <tr>
        <td><%= user_location.userID %></td>
        <td><%= user_location.address1 %></td>
        <td><%= user_location.address2 %></td>
        <td><%= user_location.city %></td>
        <td><%= user_location.state %></td>
        <td><%= user_location.zipcode %></td>
        <td><%= link_to 'Show', user_location %></td>
        <td><%= link_to 'Edit', edit_user_location_path(user_location) %></td>
        <td><%= link_to 'Destroy', user_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Location', new_user_location_path %>
